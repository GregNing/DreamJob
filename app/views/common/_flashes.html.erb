<% if flash.any? %>
    <% user_facing_flashes.each do |key,value| %>
    <div class="alert flashbar alert-dismissable alert-<%= flash_class(key) %> <%= ' session-login-navbar' if controller_name == "sessions" %>">
    <button class="close" data-dismiss="alert">X</button>
    <%= value %>
    </div>
    <% end %>
<% end %>